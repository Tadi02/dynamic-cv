<% technologies.forEach(function(tech){ %>
    <tr id="<%= tech.id %>">
        <td class="techName"><%= tech.name %></td>
        <td class="centered"><a data-toggle="modal" data-target="#editTechModal" class="updateTech" href="#"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></td>
        <td class="centered"><a href="#" class="delTech"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></a></td>
    </tr>
<% }); %>